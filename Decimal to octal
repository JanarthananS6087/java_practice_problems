package com.company;
import java.util.*;
public class decimaltooctal {
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        int decimal=sc.nextInt();
        int array[]=new int[20];
        int i=0;
        while(decimal!=0)
        {
            int temp=decimal%8;
            array[i++]=temp;
            decimal/=8;
        }
        for(int j=i-1;j>=0;j--)
        {
            System.out.print(array[j]);
        }
    }
}
