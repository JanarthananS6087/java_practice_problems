package com.company;
import java.util.*;
public class Remove_space_str {
    public static void main(String[] args)
    {
        Scanner sc=new Scanner(System.in);
        String s= sc.nextLine();
        char[] str=s.toCharArray();
        int j=0;
        for(int i=0;i<str.length;i++)
        {
            if( (str[i]!= ' ') && (str[i]!= '\t'))
            {
                System.out.print(str[i]);
            }
        }
        System.out.println();
    }
}
